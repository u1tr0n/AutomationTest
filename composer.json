{
  "name": "u1tr0n/automation_test",
  "type": "symfony-bundle",
  "description": "automate run commands manager",
  "keywords": ["Symfony", "bundle", "automation", "run commands", "commands", "ci/di"],
  "homepage": "https://github.com/u1tr0n/AutomationTest",
  "license": "GNU General Public License v3.0",
  "authors": [
    {
      "name": "Illia Dulaive",
      "email": "ilya@dulaev.me",
      "homepage": "https://u1tr0n.nl/"
    }
    ],
  "require": {
    "php": " ~8.0.0 || ~8.1.0 || ~8.2.0",
    "symfony/console": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "u1tr0n\\Bundle\\automation\\" : "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "friendsofphp/php-cs-fixer": "^3.15",
    "rector/rector": "^0.15",
    "symfony/maker-bundle": "^1.48",
    "vimeo/psalm": "^5.8",
    "composer/package-versions-deprecated": "1.11.99"
  },
  "scripts": {
    "test": [
      "vendor/bin/simple-phpunit",
      "ENCODER=lcobucci vendor/bin/simple-phpunit"
    ]
  }
}